[global]
direct=1
rw=randread
;rwmixread=80
;rwmixwrite=20
bs=4k
runtime=200
ramp_time=25
ioengine=libaio
iodepth=16
group_reporting=1
thread=1
numjobs=4


[job_1]
filename=/dev/sda1
[job_2]
filename=/dev/sdb1
[job_3]
filename=/dev/sdi1
[job_4]
filename=/dev/sdd1
[job_5]
filename=/dev/sde1
[job_6]
filename=/dev/sdf1
[job_7]
filename=/dev/sdg1
[job_8]
filename=/dev/sdh1

